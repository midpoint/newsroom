<div class="container-fluid">
  <div class="row">
    <%= render "sidebar", feeds: @feeds %>

    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-0">
      <% if !@feed.error.blank? %>
        <div class="alert alert-danger">An error occured when loading this feed: <%= @feed.error %></div>
      <% end %>
      <%= render "stories/list", stories: @stories %>
    </main>
  </div>
</div>
